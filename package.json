{
  "name": "update-me",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "env PATH=\"/home/linuxbrew/.linuxbrew/opt/openjdk@17/bin:/home/linuxbrew/.linuxbrew/bin:$PATH\" react-native start",
    "start-no-cache": "env PATH=\"/home/linuxbrew/.linuxbrew/opt/openjdk@17/bin:/home/linuxbrew/.linuxbrew/bin:$PATH\" react-native start --reset-cache",
    "android": "env PATH=\"/home/linuxbrew/.linuxbrew/opt/openjdk@17/bin:/home/linuxbrew/.linuxbrew/bin:$PATH\" react-native run-android --active-arch-only",
    "lint": "eslint",
    "stop-gradlew": "cd android && env PATH=\"/home/linuxbrew/.linuxbrew/opt/openjdk@17/bin:/home/linuxbrew/.linuxbrew/bin:$PATH\" ./gradlew --stop && cd ..",
    "clean-gradlew": "cd android && env PATH=\"/home/linuxbrew/.linuxbrew/opt/openjdk@17/bin:/home/linuxbrew/.linuxbrew/bin:$PATH\" ./gradlew clean && cd ..; pkill -f gradle; rm -rf $HOME/.gradle/daemon/",
    "status-gradlew": "cd android && env PATH=\"/home/linuxbrew/.linuxbrew/opt/openjdk@17/bin:/home/linuxbrew/.linuxbrew/bin:$PATH\" ./gradlew --status && cd ..",
    "re-gradlew": "pnpm run stop-gradlew && pnpm run clean-gradlew && pnpm run status-gradlew",
    "stop-watch": "watchman watch-del '/home/anfreire/Documents/Projects/Mobile/updateMe'; watchman watch-project '/home/anfreire/Documents/Projects/Mobile/updateMe'",
    "clean-build": "rm -rf android/app/build",
    "build-rn": "env PATH=\"/home/linuxbrew/.linuxbrew/opt/openjdk@17/bin:/home/linuxbrew/.linuxbrew/bin:$PATH\" react-native build-android --mode=release",
    "build-gradle": "cd android && env PATH=\"/home/linuxbrew/.linuxbrew/opt/openjdk@17/bin:/home/linuxbrew/.linuxbrew/bin:$PATH\" ./gradlew assembleRelease",
    "build-sign": "/opt/Android/Sdk/build-tools/34.0.0/apksigner sign --ks android/app/anfreire_key.keystore --ks-key-alias anfreire_key --out updateMe.apk android/app/build/outputs/apk/release/updateMe.apk",
    "build": "pnpm run build-clean && pnpm run build-rn && pnpm run build-gradle && cd /home/anfreire/Documents/Projects/Mobile/updateMe && pnpm run build-sign",
    "publish": "/home/anfreire/Documents/Projects/Mobile/updateMe/push_release.sh"
  },
  "dependencies": {
    "@pchmn/expo-material3-theme": "^1.3.2",
